{
  "name": "gravity-team-website",
  "version": "1.0.0",
  "description": "Gravity Team - Advanced Crypto Trading Solutions Website",
  "main": "index.html",
  "scripts": {
    "dev": "npx tailwindcss -i ./src/input.css -o ./src/output.css --watch",
    "build": "npx tailwindcss -i ./src/input.css -o ./src/output.css --minify",
    "start": "npm run build && npx serve .",
    "serve": "npx serve ."
