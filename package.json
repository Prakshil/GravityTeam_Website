  "keywords": [
    "crypto",
    "trading",
    "market-making",
    "tailwindcss",
    "gsap",
    "responsive"
  ],
  "author": "Prakshil Patel",
  "license": "MIT",
  "devDependencies": {
    "serve": "^14.2.4",
    "tailwindcss": "^3.4.17"
  },
  "dependencies": {
    "@tailwindcss/forms": "^0.5.10",
    "@tailwindcss/typography": "^0.5.16"
  }
}
